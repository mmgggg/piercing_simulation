<div class="container-fruid">
  <h1> Image Posts</h1>
 
  <div class="row">
    <div class="col-4 mx-auto">
      <%= form_with model: @coordinate_image, html: { method: :post, multipart: true },local: true do |f| %>
        <div class="form-group">
          <%= f.label :image %>
          <%= f.file_field :image %>
        </div>
        <div class="form-group">
          <%= f.label :tag_list %>
          <%= f.text_field :tag_list, value: @coordinate_image.tag_list.join(',')%>
        </div>
        <div class="col-4 mx-auto">
          <%= f.submit "Posts", class: "btn btn-secondary" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
